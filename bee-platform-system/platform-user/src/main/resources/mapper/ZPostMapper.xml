<mapper namespace="com.bee.platform.user.authority.dao.mapper.ZPostMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.bee.platform.user.entity.ZPost">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="department_id" property="departmentId"/>
